WP_ENV=development

# Database settings
DB_HOST=localhost
DB_NAME=database_name
DB_USER=database_user
DB_PASSWORD=database_password
DB_CHARSET=utf8mb4
DB_PREFIX=

# Changing the site URL
WP_HOME=http://example.org
WP_SITEURL=${WP_HOME}
WP_TITLE=Example
WP_ADMIN=webmaster
WP_ADMIN_PASSWORD=webmaster
WP_ADMIN_EMAIL=webmaster@example.org
WPLANG=en_US

# Performance tweaks
WP_CACHE=false
WP_MEMORY_LIMIT=128M
WP_MAX_MEMORY_LIMIT=256M

# Additional tweaks
DISALLOW_FILE_EDIT=true
WP_ALLOW_REPAIR=false
AUTOMATIC_UPDATER_DISABLED=true

# WordPress security keys and salts
